<h2>List Subjects</h2>
<%= search_form_for @q, url: subjects_emails_path do |f| %>
  <%= f.label :email_date_eq, 'Date' %>
  <%= f.date_field :email_date_eq %>
  <%= f.submit 'Search' %>
<% end %>

<% if @subjects.empty? %>
  <p>No emails were found.</p>
<% else %>
  <ul>
    <% @subjects.each do |subject| %>
      <li><%= subject %></li>
    <% end %>
  </ul>
<% end %>
<%= link_to 'Dashboard', dashboard_emails_path %>
